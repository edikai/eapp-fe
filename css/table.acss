
.page-section-demo {
  padding: 20rpx;
  height: 100%;
}
/* 标题 */
.page-section-title view {
  background-color: #49a9ee;
  width: 39%;
  color: #fff;
  font-size: 20px;
}
/* 标题行显示 */
.page-section-inline view {
  display: inline-block;
}
/* 竖虚线 */
.first-title {
  border-right: 1px dashed #fff;
}
/* 竖虚线 */
.first-item {
  border-right: 1px dashed #000;
}

/* 具体列表数据 */
.scroll-view-item {
  padding: 16rpx 32rpx;
  width: 39%;
  font-size: 18px;
}


